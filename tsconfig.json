{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es2015",
      "es2015.iterable",
      "dom",
      "es2017.object",
      "es2016"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve",
    "noImplicitAny": false,
    "sourceMap": true,
    "keyofStringsOnly": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
